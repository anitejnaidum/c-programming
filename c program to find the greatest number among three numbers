#include <stdio.h>
int main()
{
    int a,b,c;
    printf("enter the three numbers: ");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b>c)
    printf("%d is greater than %d,%d",a,b,c);
    else if(a<b>c)
    printf("%d is greater than %d,%d",b,a,c);
    else(a<b<c);
    printf("%d is greater than %d,%d",c,a,b);
    return 0;
}
